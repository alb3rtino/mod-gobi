{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Order Schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
      "id": {
        "type": "string",
        "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$",
        "description": "The purchase order line item id"
    }
  },
  "required": [
    "id"
  ]
}
